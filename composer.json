{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "redbeanphp",
        "version": "master",
        "dist": {
          "url": "http://www.redbeanphp.com/downloads/RedBeanPHP4_3_2.tar.gz",
          "type": "tar"
        }
      }
    }
  ],

  "require": {
    "ext-mbstring": "*",
    "slim/slim": "2.*",
    "redbeanphp": "master",
    "robmorgan/phinx": "*"
  },

  "require-dev": {
    "phpunit/phpunit": "4.3.*"
  },

  "autoload": {
    "files": [
      "vendor/redbeanphp/rb.php"
    ]
  },
  "minimum-stability": "dev",
  "config": {
    "secure-http": false
  }
}
